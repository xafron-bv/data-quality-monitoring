{
  "title": "Data Quality Detection System Documentation",
  "description": "Comprehensive technical reference for the multi-method data quality monitoring system",
  "author": "Data Quality Team",
  "language": "en",
  "gitbook": ">=3.0.0",
  "plugins": [
    "theme-api",
    "mermaid-gb3",
    "expandable-chapters",
    "search-pro",
    "code",
    "copy-code-button",
    "page-toc-button",
    "back-to-top-button",
    "versions-select",
    "github",
    "edit-link",
    "prism",
    "-highlight"
  ],
  "pluginsConfig": {
    "theme-api": {
      "theme": "dark"
    },
    "github": {
      "url": "https://github.com/your-org/detection-system"
    },
    "edit-link": {
      "base": "https://github.com/your-org/detection-system/edit/main/docs",
      "label": "Edit This Page"
    },
    "versions": {
      "options": [
        {
          "value": "latest",
          "text": "Latest"
        },
        {
          "value": "v1.0",
          "text": "v1.0"
        }
      ]
    },
    "page-toc-button": {
      "maxTocDepth": 2,
      "minTocSize": 2
    }
  },
  "styles": {
    "website": "styles/website.css"
  },
  "variables": {
    "version": "1.0.0"
  }
}